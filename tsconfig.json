{
  "include": ["remix.env.d.ts", "**/*.ts", "**/*.tsx"],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "~/private/*": ["./private/*"],
      "~/*": ["src/*"]
    },
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "target": "ESNext",
    "isolatedModules": true
  }
}
